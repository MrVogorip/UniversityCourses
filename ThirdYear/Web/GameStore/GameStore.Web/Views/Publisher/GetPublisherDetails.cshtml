@model GameStore.Web.ViewModel.PublisherViewModel

@{
    ViewData["Title"] = "Get Publisher Details";
}

<hr />
<div class="container">
    <div class="card">
        <div class="card-header">
            Publisher: @Model.CompanyName
        </div>
        <div class="card-body">
            <h6 class="card-text">Company Name: @Model.CompanyName</h6>
            <h6 class="card-text">Description: @Model.Description</h6>
            <h6 class="card-text">Home Page: @Model.HomePage</h6>
        </div>
    </div>
</div>

